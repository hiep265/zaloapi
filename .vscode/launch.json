{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Zalo API",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/src/index.js",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "restart": true,
      "runtimeExecutable": "node",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outputCapture": "std"
    },
    {
      "name": "Debug Zalo API (Nodemon)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/nodemon/bin/nodemon.js",
      "args": ["${workspaceFolder}/src/index.js"],
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "restart": false,
      "runtimeExecutable": "node",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outputCapture": "std"
    },
    {
      "name": "Debug Current File",
      "type": "node",
      "request": "launch",
      "program": "${file}",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "restart": false,
      "runtimeExecutable": "node",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
